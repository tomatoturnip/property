<h1>Record a new building</h1>

<%= simple_form_for @building do |f| %>
  <%= f.input :street_address, label: 'Street Address' %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zipcode %>
  <%= f.input :description %>
  <%= f.association :owner, label_method: :full_name, include_blank: true %>
  <%= f.button :submit, value: 'register' %>
<% end %>
